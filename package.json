{
  "name": "stroke-2-fill",
  "version": "1.0.0",
  "description": "A utility for Fantasticon font generation from SVG icons that are created using strokes",
  "engines": {
    "pnpm": ">=8"
  },
  "bin": {
    "stroke2fill": "lib/cli.js"
  },
  "scripts": {
    "build": "pnpm exec babel src --out-dir lib --extensions \".js\" --copy-files --presets @babel/preset-flow && prettier --write \"lib/**/*.js\"",
    "flow": "flow",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "prepublishOnly": "pnpm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "flow check"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fantasticon": "^2.0.0",
    "handlebars": "^4.7.7",
    "minimist": "^1.2.8",
    "oslllo-svg-fixer": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/preset-flow": "^7.27.1",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@prettier/plugin-hermes": "^0.1.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-ft-flow": "^3.0.11",
    "flow-bin": "^0.296.0",
    "flow-typed": "^4.1.1",
    "hermes-eslint": "^0.33.3",
    "prettier": "^3.7.4"
  }
}
